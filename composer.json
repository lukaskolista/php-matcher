{
    "name": "coduo/php-matcher",
    "type": "library",
    "keywords": ["json", "matcher", "tests"],
    "license": "MIT", 
    "authors": [
        {
            "name": "Michał Dąbrowski",
            "email": "dabrowski@brillante.pl"
        },
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "coduo/php-to-string": "~2.0",
        "symfony/property-access": "~2.3",
        "symfony/expression-language": "~2.4",
        "doctrine/lexer": "1.0.*",
        "openlss/lib-array2xml": "0.0.9"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "Coduo\\PHPMatcher": "src/",
            "Coduo\\PHPMatcher\\Tests": "tests/"
        },
        "files": ["match.php"]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}
